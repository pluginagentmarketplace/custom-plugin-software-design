{
  "name": "software-design-assistant",
  "version": "2.0.0",
  "displayName": "Software Design & Developer Roadmaps",
  "description": "Software design plugin with agents for patterns, principles, and clean code practices",
  "author": "Custom Plugin Marketplace",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/pluginagentmarketplace/custom-plugin-software-design",
  "keywords": [
    "developer-roadmap",
    "software-design",
    "learning-paths",
    "design-patterns",
    "architecture",
    "backend",
    "frontend",
    "mobile",
    "devops",
    "ai-ml",
    "database",
    "skills"
  ],
  "agents": [
    "./agents/01-design-principles.md",
    "./agents/02-design-patterns.md",
    "./agents/03-clean-code.md",
    "./agents/04-refactoring.md",
    "./agents/05-domain-driven.md",
    "./agents/06-testing-design.md",
    "./agents/07-architecture-patterns.md"
  ],
  "commands": [
    {
      "id": "learn",
      "name": "/learn",
      "description": "Start a structured learning path for any developer role",
      "file": "./commands/learn.md"
    },
    {
      "id": "explore",
      "name": "/explore",
      "description": "Explore available roles and their roadmaps",
      "file": "./commands/explore.md"
    },
    {
      "id": "assess",
      "name": "/assess",
      "description": "Self-assess your knowledge in specific skills",
      "file": "./commands/assess.md"
    },
    {
      "id": "roadmap",
      "name": "/roadmap",
      "description": "View detailed roadmap for a specific role",
      "file": "./commands/roadmap.md"
    }
  ],
  "skills": [
    "./skills/solid-principles/SKILL.md",
    "./skills/design-patterns/SKILL.md",
    "./skills/clean-code/SKILL.md",
    "./skills/refactoring/SKILL.md",
    "./skills/domain-driven-design/SKILL.md",
    "./skills/tdd-practices/SKILL.md",
    "./skills/architecture-patterns/SKILL.md"
  ],
  "hooks": {
    "file": "./hooks/hooks.json"
  }
}